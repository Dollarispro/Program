        Private Sub button49_Click(ByVal sender As Object, ByVal e As EventArgs)
            MessageBox.Show("FiFA", "Takes a while cause I use a for loop cause I'm lazy.")
            Dim i As Integer
            For i = &H3876 To &H3C5E - 1
                Me.Console.SetMemory(DirectCast(i, UInt32), New Byte() { &HFF })
                Thread.Sleep(10)
            Next i
        End Sub

