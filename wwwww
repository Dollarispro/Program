        End Sub

        Private Sub devModeToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            Do While (MyBase.Height <= 500)
                MyBase.Height += 1
            Loop
        End Sub

        Private Sub disableWeaponsToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.WriteInt32((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H10), &H81)
            If Not Me.StealthMode Then
                Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""^3Weapons: ^1Off""" })
            End If
        End Sub

        Protected Overrides Sub Dispose(ByVal disposing As Boolean)
            If (disposing AndAlso (Not Me.components Is Nothing)) Then
                Me.components.Dispose
            End If
            MyBase.Dispose(disposing)
        End Sub

        Private Sub DoHeart_Elapsed(ByVal sender As Object, ByVal e As ElapsedEventArgs)
            If (Me.HeartClient > &H11) Then
                Me.HeartClient = 0
            End If
            Me.setText(Me.HeartClient, Me.DoHearText(Me.HeartClient), Me.DoText, 1, 2!, 0!, 0!, 120, 5, &HFF, 0, 0, &HFF)
            Me.HeartClient += 1
        End Sub

        Private Sub enableWeaponsToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.WriteInt32((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H10), 0)
            If Not Me.StealthMode Then
                Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""^3Weapons: ^2On""" })
            End If
        End Sub

        Private Sub exitToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            MyBase.Close
        End Sub

        Private Sub Form1_Closing(ByVal sender As Object, ByVal e As CancelEventArgs)
            Me.Console.xbCon.DebugTarget.DisconnectAsDebugger
        End Sub

        Private Sub Form1_Load(ByVal sender As Object, ByVal e As EventArgs)
            MyBase.Height = &H191
        End Sub

        Public Sub FreezePlayer(ByVal Client As Integer)
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H5687), New Byte() { 60 })
        End Sub

        Private Sub freezeToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.FreezePlayer(Me.listBox1.SelectedIndex)
            If Not Me.StealthMode Then
                Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""^3mFlag: ^7Froze""" })
            End If
        End Sub

        Private Sub freezeWithLookToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.AND_UInt32((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H5684), 120)
            If Not Me.StealthMode Then
                Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""^3mFlag: ^7Froze with look""" })
            End If
        End Sub

        Public Function getGamertag(ByVal Client As Integer) As String
            Dim memory As Byte() = Me.Console.GetMemory((Me.getPlayerState(Client) + &H5534), 20)
            memory.Reverse(Of Byte)()
            Return Encoding.ASCII.GetString(memory)
        End Function

        Public Function getGentity(ByVal Client As Integer) As UInt32
            Return DirectCast((&H833BF7C0 + (Client * &H31C)), UInt32)
        End Function

        Public Function getMaterialIndex(ByVal material As String) As UInt32
            Return Me.Console.Call(HElems.material, New Object() { material })
        End Function

        Public Function getPlayerIndexForName(ByVal Name As String) As Integer
            Dim i As Integer
            For i = 0 To &H12 - 1
                If Me.getGamertag(i).Contains(Name) Then
                    Return i
                End If
            Next i
            Return 0
        End Function

        Public Function getPlayerState(ByVal Client As Integer) As UInt32
            Return DirectCast((-2091630192 + (Client * &H57F8)), UInt32)
        End Function

        Public Function GetStringTableName(ByVal index As Integer) As String
            Return (Me.Console.ReadString(Me.Console.ReadUInt32(DirectCast((-2104040836 + (index * &H12)), UInt32))) & ChrW(10))
        End Function

        Public Sub GiveKillStreaks(ByVal Client As Integer)
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H437), New Byte() { 1 })
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H432), New Byte() { 1 })
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H42E), New Byte() { 1 })
        End Sub

        Public Sub GiveWeapon(ByVal Client As Integer, ByVal WeaponName As String, ByVal Akimbo As Integer, ByVal Camo As Integer)
            Dim num As Integer = CInt(Me.Console.Call(&H823A9DC8, New Object() { WeaponName }))
            Me.Console.Call(&H823AB060, New Object() { Me.getPlayerState(Client), num, Akimbo, Camo })
            Dim str As String = ("h " & num.ToString)
            Me.Console.Call(&H8242D730, New Object() { Client, 1, str })
            Me.Console.Call(&H82310CE8, New Object() { Me.getGentity(Client), num, 0, &H3E7, 1 })
        End Sub

