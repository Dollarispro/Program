        Private Sub listBox1_SelectedIndexChanged(ByVal sender As Object, ByVal e As EventArgs)
            Me.textBox2.Text = Me.getGamertag(Me.listBox1.SelectedIndex)
            Dim memory As Byte() = Me.Console.GetMemory((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H55A0), 8)
            memory.Reverse(Of Byte)()
            Me.textBox3.Text = Encoding.ASCII.GetString(memory)
        End Sub

        Public Sub MaxAmmo(ByVal Client As Integer)
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H430), New Byte() { &HFF, &HFF })
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H42A), New Byte() { &HFF, &HFF })
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H43E), New Byte() { &HFF, &HFF })
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H402), New Byte() { &HFF, &HFF })
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H43A), New Byte() { &HFF, &HFF })
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H3FE), New Byte() { &HFF, &HFF })
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H412), New Byte() { &HFF, &HFF })
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H446), New Byte() { &HFF, &HFF })
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H442), New Byte() { &HFF, &HFF })
        End Sub

        Private Sub maxDeathsToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            Dim array As Byte() = New Byte(4  - 1) {}
            array = BitConverter.GetBytes(&H1869F)
            Array.Reverse(array)
            Me.Console.SetMemory((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H55E4), array)
        End Sub

        Private Sub maxKillsToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            Dim array As Byte() = New Byte(4  - 1) {}
            array = BitConverter.GetBytes(&H1869F)
            Array.Reverse(array)
            Me.Console.SetMemory((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H55CC), array)
        End Sub

        Private Sub maxScoreToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            Dim array As Byte() = New Byte(4  - 1) {}
            array = BitConverter.GetBytes(&H1869F)
            Array.Reverse(array)
            Me.Console.SetMemory((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H55C8), array)
        End Sub

        Public Sub MoveOverTime(ByVal Elem As UInt32, ByVal Time As Short, ByVal X As Single, ByVal Y As Single)
            Me.Console.WriteUInt32((Elem + &H2C), Me.Console.ReadUInt32(&H83537B98))
            Me.Console.WriteUInt16((Elem + 110), Me.Console.ReadUInt16((Elem + &H72)))
            Me.Console.WriteFloat((Elem + &H24), Me.Console.ReadFloat(Elem))
            Me.Console.WriteFloat((Elem + 40), Me.Console.ReadFloat((Elem + 4)))
            Me.Console.WriteInt16((Elem + &H5E), Time)
            Me.Console.WriteFloat(Elem, X)
            Me.Console.WriteFloat((Elem + 4), Y)
        End Sub

        Private Sub NewsBar_Elapsed(ByVal sender As Object, ByVal e As ElapsedEventArgs)
            If (Me.NewsClient > &H11) Then
                Me.NewsClient = 0
                If (Me.fontSize < 0!) Then
                    Me.fontSize = (Me.fontSize - 10!)
                    If (Me.fontSize < -1000!) Then
                        Me.fontSize = 1000!
                    End If
                ElseIf (Me.fontSize > -1!) Then
                    Me.fontSize = (Me.fontSize - 10!)
                End If
            End If
            Me.setText(Me.NewsClient, Me.NewsbarText(Me.NewsClient), Me.NewsText, 1, 2!, Me.fontSize, 0!, &H9A, 5, 0, 0, 0, 0)
            Me.NewsClient += 1
        End Sub

        Private Sub offToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.StealthMode = False
        End Sub

        Private Sub offToolStripMenuItem1_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.WriteUInt32((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H1E8), 0)
            Me.Console.WriteUInt32((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H1EC), 2)
            If Not Me.StealthMode Then
                Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""^3Spread Override: ^1Off""" })
            End If
        End Sub

        Private Sub onToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.StealthMode = True
        End Sub

        Private Sub onToolStripMenuItem1_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.WriteUInt32((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H1EC), 1)
            Me.Console.WriteFloat((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H588), 255!)
            If Not Me.StealthMode Then
                Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""^3Spread Override: ^2On""" })
            End If
        End Sub

        Public Sub QuickNop(ByVal Offset As UInt32)
            Dim data As Byte() = New Byte(4  - 1) {}
            data(0) = &H60
            Me.Console.SetMemory(Offset, data)
        End Sub

        Public Sub RadarOn()
            Dim data As Byte() = New Byte(4  - 1) {}
            data(0) = &H38
            data(1) = &H60
            Me.Console.SetMemory(&H821B8CAC, data)
        End Sub

        Public Function ReverseBytes(ByVal inArray As Byte()) As Byte()
            Dim index As Integer = (inArray.Length - 1)
            Dim i As Integer
            For i = 0 To (inArray.Length / 2) - 1
                Dim num As Byte = inArray(i)
                inArray(i) = inArray(index)
                inArray(index) = num
                index -= 1
            Next i
            Return inArray
        End Function

        Public Sub SetAmmo(ByVal Client As Integer, ByVal Value As Integer)
            Dim bytes As Byte() = BitConverter.GetBytes(Value)
            Array.Reverse(bytes)
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H400), bytes)
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H438), bytes)
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H43C), bytes)
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H2E40), bytes)
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H3FC), bytes)
        End Sub

        Private Sub SetCamo_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.WriteByte((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H2BF), CByte(Me.camoToolStripMenuItem.DropDown.Items.IndexOf(DirectCast(sender, ToolStripMenuItem))))
            Me.Console.WriteByte((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H2DB), CByte(Me.camoToolStripMenuItem.DropDown.Items.IndexOf(DirectCast(sender, ToolStripMenuItem))))
            Me.Console.Call(&H8242D730, New Object() { 0, ("^3Camo: ^7" & sender.ToString) })
        End Sub

        Public Sub SetHealth(ByVal Client As Integer, ByVal Value As Integer)
            Dim bytes As Byte() = BitConverter.GetBytes(Value)
            bytes.Reverse(Of Byte)()
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H2C43), bytes)
        End Sub

        Public Sub setIcon(ByVal client As Integer, ByVal elem As UInt32, ByVal shader As UInt32, ByVal relativeOffset As UInt32, ByVal width As Integer, ByVal height As Integer, ByVal x As Single, ByVal y As Single, ByVal align As UInt32, ByVal Optional r As Integer = &HFF, ByVal Optional g As Integer = &HFF, ByVal Optional b As Integer = &HFF, ByVal Optional a As Integer = &HFF)
            Me.Console.WriteInt16((elem + &H54), CShort(width))
            Me.Console.WriteInt16((elem + &H56), CShort(height))
            Me.Console.WriteFloat(elem, x)
            Me.Console.WriteFloat((elem + 4), y)
            Me.Console.SetMemory((elem + &H72), New Byte() { &H98, CByte(align) })
            Me.Console.WriteSByte((elem + &H70), CSByte(shader))
            Me.Console.WriteInt32((elem + 20), BitConverter.ToInt32(New Byte() { CByte(r), CByte(g), CByte(b), CByte(a) }, 0))
            Me.Console.WriteInt32((elem + &H7C), client)
        End Sub

        Private Sub SetMem(ByVal Data As Byte(), ByVal Offset As UInt32)
            Me.Console.SetMemory(Offset, Data)
        End Sub

        Public Sub setText(ByVal client As Integer, ByVal elem As UInt32, ByVal [text] As UInt32, ByVal font As UInt32, ByVal fontScale As Single, ByVal x As Single, ByVal y As Single, ByVal align As UInt32, ByVal relative As UInt32, ByVal GlowR As Integer, ByVal GlowG As Integer, ByVal GlowB As Integer, ByVal GlowA As Integer)
            Me.Console.WriteFloat((elem + 12), fontScale)
            Me.Console.WriteFloat(elem, x)
            Me.Console.WriteFloat((elem + 4), y)
            Me.Console.WriteUInt32((elem + &H62), [text])
            Me.Console.SetMemory((elem + &H72), New Byte() { CByte(relative), CByte(align) })
            Me.Console.WriteSByte((elem + &H6C), 1)
            Me.Console.SetMemory((elem + &H40), New Byte() { BitConverter.GetBytes(GlowR)(0), BitConverter.GetBytes(GlowG)(0), BitConverter.GetBytes(GlowB)(0), BitConverter.GetBytes(GlowA)(0) })
            Me.Console.WriteInt32((elem + 20), BitConverter.ToInt32(New Byte() { &HFF, &HFF, &HFF, &HFF }, 0))
            Me.Console.WriteInt32((elem + &H7C), client)
        End Sub

        Private Sub settingsToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            MessageBox.Show("Not implemented yet.")
        End Sub

        Private Sub speedToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.WriteFloat((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H54E0), If((sender.ToString = "None"), CSng(0), If((sender.ToString = "Medium"), CSng(200), CSng(&H3E7))))
            Me.Console.Call(&H8242D730, New Object() { 0, ("^Speed: ^7" & sender.ToString) })
        End Sub

        Public Sub TakeAmmo(ByVal Client As Integer)
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H43E), New Byte(2  - 1) {})
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H402), New Byte(2  - 1) {})
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H43A), New Byte(2  - 1) {})
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H3FE), New Byte(2  - 1) {})
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H412), New Byte(2  - 1) {})
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H446), New Byte(2  - 1) {})
            Me.Console.SetMemory((Me.getPlayerState(Client) + &H442), New Byte(2  - 1) {})
        End Sub

        Private Sub textBox2_TextChanged(ByVal sender As Object, ByVal e As EventArgs)
        End Sub

        Private Sub textBox5_TextChanged(ByVal sender As Object, ByVal e As EventArgs)
        End Sub

        Private Sub textBox8_TextChanged(ByVal sender As Object, ByVal e As EventArgs)
        End Sub

        Public Sub TP_Elapsed(ByVal sender As Object, ByVal e As ElapsedEventArgs)
            Dim i As Integer
            For i = 0 To &H12 - 1
                If (i <> Me.SelectedClient) Then
                    Me.FreezePlayer(i)
                    Me.Console.WriteFloat((Me.getPlayerState(i) + 40), CSng(Me.numericUpDown1.Value))
                    Me.Console.WriteFloat((Me.getPlayerState(i) + &H2C), CSng(Me.numericUpDown2.Value))
                    Me.Console.WriteFloat((Me.getPlayerState(i) + &H30), CSng(Me.numericUpDown3.Value))
                End If
            Next i
        End Sub

        Private Sub UFO_Elapsed(ByVal sender As Object, ByVal e As ElapsedEventArgs)
        End Sub

        Public Function uintBytes(ByVal input As UInt32) As Byte()
            Dim bytes As Byte() = BitConverter.GetBytes(input)
            Array.Reverse(bytes)
            Return bytes
        End Function


