        Private Sub Auto_Elapsed(ByVal sender As Object, ByVal e As ElapsedEventArgs)
            Me.Console.Call(&H8242D730, New Object() { -1, 0, ("; """ & Me.textBox9.Text & """") })
            Me.Console.Call(&H8242D730, New Object() { -1, 0, ("< """ & Me.textBox10.Text & """") })
        End Sub

        Private Sub autoEditToolStripMenuItem_Click(ByVal sender As Object, ByVal e As EventArgs)
            MessageBox.Show("Not implemented yet.")
        End Sub

        Public Function Bo2Buttons(ByVal Client As Integer) As Integer
            Return BitConverter.ToInt32(Me.Console.GetMemory((Me.getPlayerState(Client) + &H568C), 4), 0)
        End Function

        Private Sub button1_Click_1(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.Connect
            If Me.Console.activeConnection Then
                MessageBox.Show("Connected", "FiFA")
            End If
        End Sub

        Private Sub button10_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.SetMemory((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H5534), Encoding.ASCII.GetBytes(Me.textBox2.Text))
        End Sub

        Private Sub button11_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.SetMemory((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H55A0), Encoding.ASCII.GetBytes(Me.textBox3.Text))
        End Sub

        Private Sub button12_Click(ByVal sender As Object, ByVal e As EventArgs)
            If (BitConverter.ToInt32(Me.Console.GetMemory((Me.getGentity(Me.listBox1.SelectedIndex) + &H1A9), 4), 0) <> 0) Then
                If Not Me.StealthMode Then
                    Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""^3God Mode: ^2On""" })
                End If
                Me.Console.SetMemory((Me.getGentity(Me.listBox1.SelectedIndex) + &H1A9), BitConverter.GetBytes(0))
            Else
                If Not Me.StealthMode Then
                    Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""^3God Mode: ^1Off""" })
                End If
                Dim bytes As Byte() = BitConverter.GetBytes(&H6400)
                Array.Reverse(bytes)
                Me.Console.SetMemory((Me.getGentity(Me.listBox1.SelectedIndex) + &H1A9), bytes)
            End If
        End Sub

        Private Sub button13_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, Me.textBox1.Text })
        End Sub

        Private Sub button14_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.Call(&H823FF408, New Object() { 0, Me.textBox4.Text })
        End Sub

        Private Sub button15_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.numericUpDown1.Value = CDec(Me.Console.ReadFloat((Me.getPlayerState(Me.listBox1.SelectedIndex) + 40)))
            Me.numericUpDown2.Value = CDec(Me.Console.ReadFloat((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H2C)))
            Me.numericUpDown3.Value = CDec(Me.Console.ReadFloat((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H30)))
        End Sub

        Private Sub button16_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.Console.WriteFloat((Me.getPlayerState(Me.listBox1.SelectedIndex) + 40), CSng(Me.numericUpDown1.Value))
            Me.Console.WriteFloat((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H2C), CSng(Me.numericUpDown2.Value))
            Me.Console.WriteFloat((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H30), CSng(Me.numericUpDown3.Value))
        End Sub

        Private Sub button17_Click(ByVal sender As Object, ByVal e As EventArgs)
            Dim buffer As Byte() = New Byte(4  - 1) {}
            Dim i As Integer
            For i = 0 To &H12 - 1
                If (i <> Me.listBox1.SelectedIndex) Then
                    Me.Console.WriteFloat((Me.getPlayerState(i) + 40), CSng(Me.numericUpDown1.Value))
                    Me.Console.WriteFloat((Me.getPlayerState(i) + &H2C), CSng(Me.numericUpDown2.Value))
                    Me.Console.WriteFloat((Me.getPlayerState(i) + &H30), CSng(Me.numericUpDown3.Value))
                End If
            Next i
        End Sub

        Private Sub button18_Click(ByVal sender As Object, ByVal e As EventArgs)
            If (((Me.Console.ReadInt32((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H5687)) And &H60) <> &H60) OrElse ((Me.Console.ReadInt32((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H5687)) And &H70) <> &H70)) Then
                Me.Console.SetMemory((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H5687), New Byte() { 60 })
                If Not Me.StealthMode Then
                    Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""^3Movement: ^1Off""" })
                End If
            Else
                Me.Console.WriteInt32((Me.getPlayerState(Me.listBox1.SelectedIndex) + &H5687), &H358)
                If Not Me.StealthMode Then
                    Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""^3Movement: ^2On""" })
                End If
            End If
        End Sub

        Private Sub button19_Click(ByVal sender As Object, ByVal e As EventArgs)
            Dim i As Integer
            For i = 0 To &H12 - 1
                Me.NewsbarShader(i) = Me.Console.Call(HElems.elem, New Object() { i })
                Me.NewsbarText(i) = Me.Console.Call(HElems.elem, New Object() { i })
            Next i
            Me.NewsText = Me.createText(Me.textBox6.Text)
            Me.NewsBar.Interval = 10
            Me.NewsBar.Enabled = True
            AddHandler Me.NewsBar.Elapsed, New ElapsedEventHandler(AddressOf Me.NewsBar_Elapsed)
            Me.NewsBar.Start
        End Sub

        Private Sub button2_Click(ByVal sender As Object, ByVal e As EventArgs)
            Me.listBox1.Items.Clear
            Dim i As Integer
            For i = 0 To &H12 - 1
                Me.listBox1.Items.Add(Me.getGamertag(i))
            Next i
        End Sub

