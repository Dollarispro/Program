        Private Sub button9_Click_1(ByVal sender As Object, ByVal e As EventArgs)
            If Not Me.infared(Me.listBox1.SelectedIndex) Then
                Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "2 1060 ""infrared"" 3000" })
                If Not Me.StealthMode Then
                    Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""Infared: ^2On""" })
                End If
                Me.infared(Me.listBox1.SelectedIndex) = True
            Else
                Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "2 1060 ""default"" 3000" })
                If Not Me.StealthMode Then
                    Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""Infared: ^1Off""" })
                End If
                Me.infared(Me.listBox1.SelectedIndex) = False
            End If
        End Sub

        Public Sub Cheater()
            Me.Console.Call(&H823FF408, New Object() { 0, "set bg_bobMax 0;set bg_weaponBobMax 0;set player_breath_hold_time 999;set cg_overheadNamesGlow 1 0 1 1;set cg_overheadNamesSize 2;set cg_overheadNamesMaxDist 999;set cg_overheadNamesNearDist 999;set cg_enemyNameFadeIn 0.1;set cg_enemyNameFadeOut 99999;set cg_drawThroughWalls 1;set compassMaxRange 5000;set compassShowEnemies 1;set lowAmmoWarningColor1 0 1 1 1;set lowAmmoWarningColor2 0 0 1 1;set lowAmmoWarningNoReloadColor1 1 0 1 1;set lowAmmoWarningNoReloadColor2 0 1 0 1;set player_breath_gasp_time 0;set player_breath_gasp_lerp 0;set perk_weapSwitchMultiplier 0.1;set perk_weapSpreadMultiplier 0.1;set perk_weapReloadMultiplier 0.1;set perk_weapRateMultiplier 0.1;set perk_weapAdsMultiplier 0.1;set perk_weapMeleeMultiplier 0.1;set perk_weapTossMultiplier 0.1;set perk_weapEquipmentUseMultiplier 0.1;set perk_bulletPenetrationMultiplier 0.1;set perk_killstreakReduction 999" })
        End Sub

        Private Sub checkBox1_CheckedChanged(ByVal sender As Object, ByVal e As EventArgs)
            Me.SelectedClient = Me.listBox1.SelectedIndex
            If Me.checkBox1.Checked Then
                Me.TP.Interval = 2500
                Me.TP.Enabled = True
                AddHandler Me.TP.Elapsed, New ElapsedEventHandler(AddressOf Me.TP_Elapsed)
                Me.TP.Start
            Else
                Me.TP.Stop
            End If
        End Sub

        Private Sub checkBox2_CheckedChanged(ByVal sender As Object, ByVal e As EventArgs)
            If (((Me.Console.ReadInt32(&H82495778) = &H60000000) AndAlso (Me.Console.ReadInt32(&H82495790) = &H60000000)) AndAlso (Me.Console.ReadInt32(&H824957A8) = &H60000000)) Then
                If Me.checkBox2.Checked Then
                    Me.Clantag.Interval = 10000
                    Me.Clantag.Enabled = True
                    AddHandler Me.Clantag.Elapsed, New ElapsedEventHandler(AddressOf Me.Clantag_Elapsed)
                    Me.Clantag.Start
                Else
                    Me.Clantag.Stop
                End If
            Else
                MessageBox.Show("Error", "You need to remove cheat protection.")
                If Me.checkBox2.Checked Then
                    Me.checkBox2.Checked = False
                End If
            End If
        End Sub

        Private Sub checkBox3_CheckedChanged(ByVal sender As Object, ByVal e As EventArgs)
            If Me.checkBox3.Checked Then
                Me.numericUpDown4.Enabled = True
                Me.numericUpDown5.Enabled = True
                Me.numericUpDown6.Enabled = True
                Me.numericUpDown7.Enabled = True
                Me.numericUpDown8.Enabled = True
                Me.numericUpDown9.Enabled = True
                Me.numericUpDown10.Enabled = True
                Me.numericUpDown11.Enabled = True
                Me.numericUpDown12.Enabled = True
                Me.button49.Enabled = True
            Else
                Me.numericUpDown4.Enabled = False
                Me.numericUpDown5.Enabled = False
                Me.numericUpDown6.Enabled = False
                Me.numericUpDown7.Enabled = False
                Me.numericUpDown8.Enabled = False
                Me.numericUpDown9.Enabled = False
                Me.numericUpDown10.Enabled = False
                Me.numericUpDown11.Enabled = False
                Me.numericUpDown12.Enabled = False
                Me.button49.Enabled = False
            End If
        End Sub

        Private Sub Clantag_Elapsed(ByVal sender As Object, ByVal e As ElapsedEventArgs)
            Me.Cheater
        End Sub

