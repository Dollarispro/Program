        Private Sub button28_Click(ByVal sender As Object, ByVal e As EventArgs)
            If Me.checkBox3.Checked Then
                Dim num As UInt32 = &H8433AFF8
                Me.numericUpDown4.Value = Me.Console.ReadInt32((num + &H7DC))
                Me.numericUpDown5.Value = Me.Console.ReadInt32((num + &H7EE))
                Me.numericUpDown6.Value = Me.Console.ReadInt32((num + &H818))
                Me.numericUpDown7.Value = Me.Console.ReadInt32((num + &H338))
                Me.numericUpDown8.Value = Me.Console.ReadInt32((num + &H10A))
                Me.numericUpDown9.Value = Me.Console.ReadInt32((num + &H91A))
                Me.numericUpDown10.Value = Me.Console.ReadInt32((num + &HC0E))
                Me.numericUpDown11.Value = Me.Console.ReadInt32((num + &H620))
                Me.numericUpDown12.Value = Me.Console.ReadInt32((num + &H8D2))
            End If
        End Sub

        Private Sub button29_Click(ByVal sender As Object, ByVal e As EventArgs)
            If Me.checkBox3.Checked Then
                Dim num As UInt32 = &H8433AFF8
                Dim array As Byte() = New Byte(4  - 1) {}
                array = BitConverter.GetBytes(CInt(Me.numericUpDown4.Value))
                Array.Reverse(array)
                Me.Console.SetMemory((num + &H7DC), array)
                Me.Console.WriteInt32((num + &H7DC), CInt(Me.numericUpDown4.Value))
                array = BitConverter.GetBytes(CInt(Me.numericUpDown5.Value))
                Array.Reverse(array)
                Me.Console.SetMemory((num + &H7EE), array)
                Me.Console.WriteInt32((num + &H7EE), CInt(Me.numericUpDown5.Value))
                array = BitConverter.GetBytes(CInt(Me.numericUpDown6.Value))
                Array.Reverse(array)
                Me.Console.SetMemory((num + &H818), array)
                array = BitConverter.GetBytes(CInt(Me.numericUpDown7.Value))
                Array.Reverse(array)
                Me.Console.SetMemory((num + &H338), array)
                array = BitConverter.GetBytes(CInt(Me.numericUpDown8.Value))
                Array.Reverse(array)
                Me.Console.SetMemory((num + &H10A), array)
                array = BitConverter.GetBytes(CInt(Me.numericUpDown9.Value))
                Array.Reverse(array)
                Me.Console.SetMemory((num + &H91A), array)
                array = BitConverter.GetBytes(CInt(Me.numericUpDown10.Value))
                Array.Reverse(array)
                Me.Console.SetMemory((num + &HC0E), array)
                array = BitConverter.GetBytes(CInt(Me.numericUpDown11.Value))
                Array.Reverse(array)
                Me.Console.SetMemory((num + &H620), array)
                array = BitConverter.GetBytes(CInt(Me.numericUpDown12.Value))
                Array.Reverse(array)
                Me.Console.SetMemory((num + &H8D2), array)
            End If
        End Sub

