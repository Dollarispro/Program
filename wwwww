        Private Sub button24_Click(ByVal sender As Object, ByVal e As EventArgs)
            If (BitConverter.ToInt32(Me.Console.GetMemory((Me.getGentity(Me.listBox1.SelectedIndex) + &H11C), 4), 0) = 0) Then
                If Not Me.StealthMode Then
                    Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""^3Invsblity: ^1Off""" })
                End If
                Me.Console.SetMemory((Me.getPlayerState(Me.listBox1.SelectedIndex) + &HFC), New Byte(4  - 1) {})
                Me.Console.SetMemory((Me.getGentity(Me.listBox1.SelectedIndex) + &H11C), New Byte(4  - 1) {})
            Else
                If Not Me.StealthMode Then
                    Me.Console.Call(&H8242D730, New Object() { Me.listBox1.SelectedIndex, 0, "; ""^3Invsblity: ^2On""" })
                End If
                Dim data As Byte() = New Byte(4  - 1) {}
                data(3) = &H20
                Me.Console.SetMemory((Me.getPlayerState(Me.listBox1.SelectedIndex) + &HFC), data)
                Me.Console.SetMemory((Me.getGentity(Me.listBox1.SelectedIndex) + &H11C), New Byte(4  - 1) {})
            End If
        End Sub

